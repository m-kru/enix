[
	{
		"Name": "Default",
		"Attribute": {
			"Regex": "\\b(access|add-enable|atomic|byte-write-enable|clear|delay|enable-init-value|enable-reset-value|init-value|in-trigger|masters|out-trigger|range|read-latency|read-value|reset|reset-value|size|virtual|width)\\b",
			"PositiveLookBehind": "(^|;)\\s*",
			"PositiveLookAhead": "\\s*="
		},
		"Keyword": {
			"Regex": "\\b(const|import|type)\\b"
		},
		"Number": {
			"Regex": "(?i)\\b((0b[01_]+)|(0o[0-7_]+)|(0x[\\da-f_]+)|(\\d+(\\.\\d+)?(e[+-]?\\d+)?))\\b"
		},
		"Type": {
			"Regex": "\\b(blackbox|block|bus|config|group|irq|mask|memory|param|proc|return|status|stream|static)\\b"
		},
		"Value": {
			"Regex": "\\b(false|true)\\b"
		}
	},
	{
		"Name": "Line Comment",
		"Style": "Comment",
		"Start": {
			"Regex": "#"
		},
		"End": {
			"Regex": "$"
		},
		"Bold": {
			"Regex": "(?i)(todo|note|xxx):"
		}
	},
	{
		"Name": "String",
		"Style": "String",
		"Start": {
			"Regex": "\""
		},
		"End": {
			"Regex": "\""
		}
	}
]
