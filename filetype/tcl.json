[
	{
		"Name": "Default",
		"Attribute": {
			"Regex": "-\\w+",
			"PositiveLookBehind": "\\s+",
			"PositiveLookAhead": "(\\b)"
		},
		"Keyword": {
			"Regex": "\\b(break|catch|continue|default|else|elseif|eq|error|exit|for|foreach|if|namespace|ne|ni|proc|return|switch|variable|while)\\b"
		},
		"Number": {
			"Regex": "\\b(0(b|B|o|O|x|X)?)?\\d+(\\.\\d+)?((e|E)-?\\d+)?\\b"
		},
		"Variable": {
			"Regex": "\\$(\\w|:)+"
		},
		"Operator": {
			"Regex": "\\[|\\]"
		}
	},
	{
		"Name": "Line Comment",
		"Style": "Comment",
		"Start": {
			"Regex": "#"
		},
		"End": {
			"Regex": "$"
		},
		"Bold": {
			"Regex": "(?i)(todo|note|xxx):"
		}
	},
	{
		"Name": "String",
		"Style": "String",
		"Start": {
			"Regex": "\""
		},
		"End": {
			"Regex": "\"",
			"NegativeLookBehind": "(?:[^\\\\])\\\\"
		},
		"Variable": {
			"Regex": "\\$(\\w|:)+",
			"NegativeLookBehind": "\\\\"
		},
		"Meta": {
			"Regex": "\\\\(a|b|f|n|r|t|v|\"|\\[|]|\\$|\\\\)"
		},
		"Operator": {
			"Regex": "\\[|\\]",
			"NegativeLookBehind": "\\\\"
		}
	}
]
